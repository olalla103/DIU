<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" styleClass="background" stylesheets="@../../../estilos/DarkTheme.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.agenda.controller.PersonEditDialogController">
    <children>
        <GridPane layoutX="180.0" layoutY="95.0" prefHeight="270.0" prefWidth="580.0" AnchorPane.leftAnchor="10.0"
                  AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="281.0" minWidth="10.0" prefWidth="195.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="399.0" minWidth="10.0" prefWidth="385.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="First Name"/>
                <Label text="Last Name" GridPane.rowIndex="1"/>
                <Label text="Street" GridPane.rowIndex="2"/>
                <Label text="Postal Code" GridPane.rowIndex="3"/>
                <Label text="City" GridPane.rowIndex="4"/>
                <Label text="Birthday" GridPane.rowIndex="5"/>
                <TextField fx:id="firstNameField" GridPane.columnIndex="1"/>
                <TextField fx:id="lastNameField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <TextField fx:id="streetField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <TextField fx:id="postalCodeField" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <TextField fx:id="cityField" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                <TextField fx:id="birthdayField" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
            </children>
        </GridPane>
        <HBox layoutX="368.0" layoutY="344.0" spacing="10.0">
            <children>
                <Button defaultButton="true" layoutX="368.0" layoutY="350.0" mnemonicParsing="false"
                        onAction="#handleOk" prefHeight="30.0" prefWidth="100.0" text="OK">
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </Button>
                <Button layoutX="473.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleCancel"
                        prefHeight="30.0" prefWidth="100.0" text="Cancel"/>
            </children>
        </HBox>
        <ProgressBar fx:id="progreso" layoutX="20.0" layoutY="352.0" prefHeight="20.0" prefWidth="222.0"
                     progress="0.0"/>
        <Text fill="WHITE" layoutX="253.0" layoutY="367.0" strokeType="OUTSIDE" strokeWidth="0.0" text="OCUPADO"/>
        <ProgressIndicator fx:id="indicator" layoutX="20.0" layoutY="322.0" progress="0.0"/>
    </children>
</AnchorPane>